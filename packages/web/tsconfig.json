{
  "compilerOptions": {
    "types": [
      "@react-router/node",
      "vite/client"
    ],
    "rootDirs": [".", "./.react-router/types"],
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "lib": [
      "ES2015",
      "DOM",
      "DOM.Iterable"
    ],
    "jsx": "react-jsx",
    "strict": true,
    "noEmit": true,
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "resolveJsonModule": true,
    "downlevelIteration": true,
    "paths": {
      "~/*": [
        "./app/*"
      ]
    },
    "baseUrl": ".",
    "forceConsistentCasingInFileNames": true
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".react-router/types/**/*"
, "tailwind.config.js"  ],
  "exclude": [
    "**/*.test.{ts,tsx}",
    "**/__tests__/**/*.{ts,tsx}",
    "node_modules"
  ]
}